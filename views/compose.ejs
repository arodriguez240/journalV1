<%- include('header') %>



    <h1>Compose</h1>

    
    <form action="/compose" method="POST">
        <div class="form-group">
            <label for="title">Title</label>
            <input autocomplete="off" class="form-control" name="newTitle" placeholder="Today's Post" type="text">
            
            <label for="Post">Post</label>
            <textarea class="form-control " placeholder="Enter today's entry" name="newPost" id="" cols="30" rows="10"></textarea>
            <button class="btn btn-info" type="submit">Post</button>
        </div>

    </form>







    <%- include('footer') -%>